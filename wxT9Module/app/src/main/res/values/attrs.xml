<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="KeyboardButton">
        <attr name="number_text" format="string"/>
        <attr name="letter_text" format="string"/>
        <attr name="number_text_size" format="dimension"/>
        <attr name="letter_text_size" format="dimension"/>
        <attr name="number_text_color" format="color"/>
        <attr name="letter_text_color" format="color"/>
        <attr name="normal_bg_drawable" format="reference"/>
        <attr name="pressed_bg_drawable" format="reference"/>
        <attr name="bg_dismiss_time" format="integer"/>
    </declare-styleable>

    <declare-styleable name="T9KeyBoard">
        <!--0:默认主题，拨号盘Tab样式; 1:通话界面拨号盘样式-->
        <attr name="theme" format="integer"/>
    </declare-styleable>

</resources>
